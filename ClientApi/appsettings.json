{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "LogSettings": {
    "DebugEnabled": false,
    "TitlePrefix": "[{Application}] ",
    "JsonBlacklist": [ "*card.number", "*card.csc", "*vtexAppToken" ],
    "HeaderBlacklist": [ "Authorization", "x-vtex-api-apptoken", "x-vtex-api-appToken", "X-Vtex-Api-Apptoken" ],
    "SeqOptions": {
      "Enabled": false,
      "MinimumLevel": "Verbose",
      "Url": "http://localhost:5341",
      "ApiKey": null
    },
    "NewRelicOptions": {
      "Enabled": false,
      "AppName": "MundiPagg Hub Vtex"
    },
    "SplunkOptions": {
      "Enabled": false,
      "MinimumLevel": "Verbose",
      "Url": "http://localhost:8088/services/collector",
      "Token": "XXXX",
      "Index": "hub.apps",
      "Application": "Hub-InternalApp-Vtex",
      "ProcessName": "StoneCo.Hub.VtexApp",
      "Company": "Mundipagg",
      "ProductVersion": "1.0.0",
      "SourceType": "_json"
    }
  }
}
